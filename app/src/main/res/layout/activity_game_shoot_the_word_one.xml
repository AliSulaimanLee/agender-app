<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="gameShootTheWordOneVM" type="com.agender.app.modules.gameshootthewordone.data.viewmodel.GameShootTheWordOneVM"/>
    </data>
    <FrameLayout android:id="@+id/frameStackairywomanshoo"
        android:layout_height="match_parent"
        android:layout_marginEnd="@dimen/_40pxh"
        android:layout_marginTop="@dimen/_48pxv"
