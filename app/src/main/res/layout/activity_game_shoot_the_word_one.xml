                            android:layout_marginEnd="@dimen/_12pxh"
                            android:layout_marginStart="@dimen/_12pxh"
                            android:layout_width="@dimen/_265pxh" style="@style/groupStylecornerRadius">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtEighty"
                                android:layout_gravity="top|center"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_71pxv"
                                android:layout_marginEnd="@dimen/_106pxh"
                                android:layout_marginStart="@dimen/_106pxh"
                                android:layout_marginTop="@dimen/_71pxv"
                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                android:text="@{gameShootTheWordOneVM.gameShootTheWordOneModel.txtEighty}"
