                                android:ellipsize="end"
                                android:gravity="center"
                                android:id="@+id/btnMateriBerikutnya"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_5pxh"
                                android:layout_marginTop="@dimen/_32pxv"
                                android:layout_width="match_parent"
                                android:paddingBottom="@dimen/_10pxv"
                                android:paddingEnd="@dimen/_30pxh"
                                android:paddingStart="@dimen/_30pxh"
                                android:paddingTop="@dimen/_10pxv"
                                android:singleLine="true"
                                android:text="@string/msg_materi_berikutn"
                                android:textAllCaps="false"
                                style="@style/btnSolidRounded_3" tools:text="@string/msg_materi_berikutn"/>
                        </LinearLayout>
                    </FrameLayout>
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnKembali"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_88pxv"
                        android:layout_marginEnd="@dimen/_17pxh"
                        android:layout_marginStart="@dimen/_17pxh"
                        android:layout_marginTop="@dimen/_28pxv"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_10pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_10pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_kembali"
                        android:textAllCaps="false"
                        style="@style/btnSolidRoundedOutline_2" tools:text="@string/lbl_kembali"/>
