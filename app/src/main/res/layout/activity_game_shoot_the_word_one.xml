                        android:layout_height="@dimen/_505pxv"
                        android:layout_marginEnd="@dimen/_17pxh"
                        android:layout_marginStart="@dimen/_17pxh"
                        android:layout_marginTop="@dimen/_26pxv" android:layout_width="@dimen/_289pxh">
