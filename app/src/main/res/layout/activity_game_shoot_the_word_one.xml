                                android:layout_gravity="center_horizontal"
                                android:layout_height="@dimen/_181pxh"
                                android:layout_marginEnd="@dimen/_82pxh"
                                android:layout_marginStart="@dimen/_82pxh"
                                android:layout_marginTop="@dimen/_33pxv"
                                android:layout_width="@dimen/_113pxh"
                                android:scaleType="fitXY"
                                imageRes="@{@drawable/img_a37b8926c9ab4}"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_a37b8926c9ab4"/>
