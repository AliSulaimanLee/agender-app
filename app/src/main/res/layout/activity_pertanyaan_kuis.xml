<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="pertanyaanKuisVM" type="com.agender.app.modules.pertanyaankuis.data.viewmodel.PertanyaanKuisVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumn"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_46pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_51">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumnarrowleft"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylecyan_1">
            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_45pxv"
                android:layout_marginEnd="@dimen/_40pxh"
                android:layout_marginStart="@dimen/_40pxh"
                android:layout_marginTop="@dimen/_33pxv"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearRowarrowleft"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_height="@dimen/_11pxh"
                        android:layout_marginBottom="@dimen/_5pxv"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_width="@dimen/_6pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft_white_a700"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft_white_a700"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtKuis"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_151pxh"
                        android:layout_weight="0.83"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{pertanyaanKuisVM.pertanyaanKuisModel.txtKuis}"
                        style="@style/txtPoppinssemibold15" tools:text="@string/lbl_kuis"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
        </LinearLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnapayangdimaks"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_40pxh"
            android:layout_marginStart="@dimen/_40pxh"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylegray_51">
            <FrameLayout android:id="@+id/frameStackgroupeighteen"
                android:layout_height="@dimen/_306pxv"
                android:layout_width="@dimen/_350pxh" style="@style/groupStylewhite_2">
                <LinearLayout
                    android:id="@+id/linearColumngroupeighteen"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_40pxv"
                    android:layout_marginEnd="@dimen/_23pxh"
                    android:layout_marginStart="@dimen/_23pxh"
                    android:layout_marginTop="@dimen/_40pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <LinearLayout
                        android:id="@+id/linearRowgroupeighteen"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_7pxh"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ProgressBar
                            android:id="@+id/progressBarGroupEighteen"
                            android:indeterminate="false"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_weight="0.94"
                            android:layout_width="0dp" android:max="100"
                            android:minHeight="@dimen/_12pxv"
                            android:progress="40"
                            android:progressDrawable="@drawable/layer_list_bg_blue_800_7f_radius_6" style="@style/Widget.AppCompat.ProgressBar.Horizontal"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTwentyFive"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_13pxh"
                            android:layout_weight="0.06"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{pertanyaanKuisVM.pertanyaanKuisModel.txtTwentyFive}"
                            style="@style/txtPoppinsmedium9" tools:text="@string/lbl_2_5"/>
                    </LinearLayout>
                    <View android:background="@color/gray_503"
                        android:id="@+id/lineLineOne"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="@dimen/_2pxv"
                        android:layout_marginEnd="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_18pxv" android:layout_width="@dimen/_300pxh"/>
                    <FrameLayout android:id="@+id/frameStackfifty"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="@dimen/_158pxv"
                        android:layout_marginEnd="@dimen/_63pxh"
                        android:layout_marginStart="@dimen/_63pxh"
                        android:layout_marginTop="@dimen/_22pxv"
                        android:layout_width="@dimen/_176pxh" style="@style/groupStylecornerRadius">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtFifty"
                            android:layout_gravity="top|center"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_42pxv"
                            android:layout_marginEnd="@dimen/_70pxh"
                            android:layout_marginStart="@dimen/_70pxh"
                            android:layout_marginTop="@dimen/_42pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{pertanyaanKuisVM.pertanyaanKuisModel.txtFifty}"
                            style="@style/txtPoppinssemibold24" tools:text="@string/lbl_50"/>
                        <FrameLayout
                            android:background="@color/red_900_7f"
                            android:id="@+id/frameGroupSixtySeven"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                    </FrameLayout>
                </LinearLayout>
                <TextView android:gravity="top|center"
                    android:id="@+id/txtApayangdimaks"
                    android:layout_gravity="bottom|center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_38pxv"
                    android:layout_marginEnd="@dimen/_66pxh"
                    android:layout_marginStart="@dimen/_66pxh"
                    android:layout_marginTop="@dimen/_38pxv"
                    android:layout_width="match_parent"
                    android:text="@{pertanyaanKuisVM.pertanyaanKuisModel.txtApayangdimaks}"
                    style="@style/txtPoppinsmedium15_1" tools:text="@string/msg_apa_yang_dimaks"/>
            </FrameLayout>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumnloremipsumdol"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_4">
                <TextView android:gravity="center_horizontal"
                    android:id="@+id/txtLoremipsumdol"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="@dimen/_8pxv"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:text="@{pertanyaanKuisVM.pertanyaanKuisModel.txtLoremipsumdol}"
                    style="@style/txtPoppinsmedium15_2" tools:text="@string/msg_pemberian_hak_h"/>
            </LinearLayout>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumnloremipsumdolOne"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_4">
                <TextView android:gravity="start"
                    android:id="@+id/txtLoremipsumdolOne"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="@dimen/_7pxv"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:text="@{pertanyaanKuisVM.pertanyaanKuisModel.txtLoremipsumdolOne}"
                    style="@style/txtPoppinsmedium15_2" tools:text="@string/msg_kesenjangan_gen"/>
            </LinearLayout>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumnloremipsumdolTwo"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_4">
                <TextView android:gravity="start"
                    android:id="@+id/txtLoremipsumdolTwo"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="@dimen/_7pxv"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:text="@{pertanyaanKuisVM.pertanyaanKuisModel.txtLoremipsumdolTwo}"
                    style="@style/txtPoppinsmedium15_2" tools:text="@string/msg_kondisi_di_mana"/>
            </LinearLayout>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumnlanguage"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_4">
                <TextView android:gravity="start"
                    android:id="@+id/txtLanguage"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="@dimen/_7pxv"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:text="@{pertanyaanKuisVM.pertanyaanKuisModel.txtLanguage}"
                    style="@style/txtPoppinsmedium15_2" tools:text="@string/msg_pemisahan_tugas"/>
            </LinearLayout>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumnloremipsumdolThree"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_4">
                <TextView android:gravity="start"
                    android:id="@+id/txtLoremipsumdolThree"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="@dimen/_7pxv"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:text="@{pertanyaanKuisVM.pertanyaanKuisModel.txtLoremipsumdolThree}"
                    style="@style/txtPoppinsmedium15_2" tools:text="@string/msg_diskriminasi_be"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowframeeightytwo"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_55pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFrameEightyTwo"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.15"
                    android:layout_width="0dp"
                    android:paddingBottom="@dimen/_9pxv"
                    android:paddingEnd="@dimen/_20pxh"
                    android:paddingStart="@dimen/_20pxh"
                    android:paddingTop="@dimen/_10pxv"
                    android:singleLine="true"
                    android:text="@{pertanyaanKuisVM.pertanyaanKuisModel.txtFrameEightyTwo}"
                    style="@style/txtRoundedOutline_1" tools:text="@string/lbl"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFrame209"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_weight="0.7"
                    android:layout_width="0dp"
                    android:paddingBottom="@dimen/_9pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_10pxv"
                    android:singleLine="true"
                    android:text="@{pertanyaanKuisVM.pertanyaanKuisModel.txtFrame209}"
                    style="@style/txtSolidRounded" tools:text="@string/lbl_jawab"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFrame208"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_9pxh"
                    android:layout_weight="0.15"
                    android:layout_width="0dp"
                    android:paddingBottom="@dimen/_9pxv"
                    android:paddingEnd="@dimen/_20pxh"
                    android:paddingStart="@dimen/_20pxh"
                    android:paddingTop="@dimen/_10pxv"
                    android:singleLine="true"
                    android:text="@{pertanyaanKuisVM.pertanyaanKuisModel.txtFrame208}"
                    style="@style/txtRoundedOutline_2" tools:text="@string/lbl2"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
