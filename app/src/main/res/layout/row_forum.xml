<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="forumRowModel" type="com.agender.app.modules.forum.data.model.ForumRowModel"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnellipsetwo"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_1">
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowellipsetwo"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_41pxh"
            android:layout_marginStart="@dimen/_41pxh"
            android:layout_marginTop="@dimen/_20pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <ImageView android:id="@+id/imageEllipseTwo"
                android:layout_height="@dimen/_29pxh"
                android:layout_width="@dimen/_29pxh"
                android:scaleType="centerCrop" circular="@{true}"
                imageRes="@{@drawable/img_ellipse2}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse2"/>
            <TextView android:ellipsize="end" android:id="@+id/txtEren"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_6pxv"
                android:layout_marginStart="@dimen/_7pxh"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_weight="0.47" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{forumRowModel.txtEren}"
                style="@style/txtIntersemibold12" tools:text="@string/lbl_eren"/>
        </LinearLayout>
        <TextView android:ellipsize="end"
            android:gravity="center_horizontal"
            android:id="@+id/txtLanguage"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_35pxh"
            android:layout_marginStart="@dimen/_41pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="match_parent"
            android:singleLine="true"
            android:text="@{forumRowModel.txtLanguage}"
            style="@style/txtPoppinsmedium14" tools:text="@string/msg_cara_menanggapi"/>
        <LinearLayout android:id="@+id/linearRowvector1"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_19pxv"
            android:layout_marginEnd="@dimen/_36pxh"
            android:layout_marginStart="@dimen/_41pxh"
            android:layout_marginTop="@dimen/_3pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <LinearLayout android:id="@+id/linearRowvector2"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_weight="0.53" android:layout_width="0dp" android:orientation="horizontal">
                <ImageView android:id="@+id/imageVector"
                    android:layout_height="@dimen/_10pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_9pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector_cyan_900_10x9"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector_cyan_900_10x9"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTen"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_weight="0.14"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{forumRowModel.txtTen}"
                    style="@style/txtPoppinsregular8" tools:text="@string/lbl_10"/>
                <ImageView android:id="@+id/imageVectorOne"
                    android:layout_height="@dimen/_10pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_10pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector_cyan_900_10x10"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector_cyan_900_10x10"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTenOne"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_weight="0.14"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{forumRowModel.txtTenOne}"
                    style="@style/txtPoppinsregular8" tools:text="@string/lbl_10"/>
                <ImageView android:id="@+id/imageVectorTwo"
                    android:layout_height="@dimen/_10pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_12pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector_cyan_900_10x12"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector_cyan_900_10x12"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTenTwo"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_weight="0.14"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{forumRowModel.txtTenTwo}"
                    style="@style/txtPoppinsregular8" tools:text="@string/lbl_10"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txt10menityangl"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_120pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_weight="0.47" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{forumRowModel.txt10menityangl}"
                style="@style/txtPoppinsregular8" tools:text="@string/msg_10_menit_yang_l"/>
        </LinearLayout>
    </LinearLayout>
</layout>
