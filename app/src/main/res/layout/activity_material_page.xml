<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="materialPageVM" type="com.agender.app.modules.materialpage.data.viewmodel.MaterialPageVM"/>
    </data>
    <FrameLayout android:id="@+id/frameStackpengenalankese"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_74pxv"
        android:layout_width="match_parent" style="@style/groupStylegray_50">
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true"
            android:id="@+id/scrollViewScrollview"
            android:layout_gravity="start" android:layout_height="match_parent"
            android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumnarrowleft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <FrameLayout android:gravity="start"
                    android:id="@+id/frameStackarrowleft"
                    android:layout_height="@dimen/_304pxv" android:layout_width="match_parent">
                    <LinearLayout android:id="@+id/linearRowarrowleft"
                        android:layout_gravity="top|start"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:orientation="horizontal" style="@style/groupStylecyan_1">
                        <ImageView android:id="@+id/imageArrowleft"
                            android:layout_height="@dimen/_11pxh"
                            android:layout_marginBottom="@dimen/_48pxv"
                            android:layout_marginStart="@dimen/_40pxh"
                            android:layout_marginTop="@dimen/_42pxv"
                            android:layout_width="@dimen/_6pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrowleft_white_a700"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft_white_a700"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtDetailmateri"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_43pxv"
                            android:layout_marginEnd="@dimen/_171pxh"
                            android:layout_marginStart="@dimen/_124pxh"
                            android:layout_marginTop="@dimen/_37pxv"
                            android:layout_weight="0.93"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_7pxh"
                            android:singleLine="true"
                            android:text="@{materialPageVM.materialPageModel.txtDetailmateri}"
                            style="@style/txtPoppinsmedium14_1" tools:text="@string/lbl_detail_materi"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageRectangleSix"
                        android:layout_gravity="bottom|center"
                        android:layout_height="@dimen/_214pxh"
                        android:layout_marginEnd="@dimen/_40pxh"
                        android:layout_marginStart="@dimen/_40pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="@dimen/_348pxh"
                        android:scaleType="fitXY"
                        cornerRadius="@{@dimen/_25pxh}"
                        imageRes="@{@drawable/img_rectangle6}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle6"/>
                </FrameLayout>
                <LinearLayout android:id="@+id/linearRowpengenalankese"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_40pxh"
                    android:layout_marginStart="@dimen/_40pxh"
                    android:layout_marginTop="@dimen/_20pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPengenalanKese"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.86"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_8pxh"
                        android:singleLine="true"
                        android:text="@{materialPageVM.materialPageModel.txtPengenalanKese}"
                        style="@style/txtPoppinssemibold15_1" tools:text="@string/msg_pengenalan_kese"/>
                    <LinearLayout android:id="@+id/linearRowvector"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_76pxh"
                        android:layout_weight="0.14"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageVector"
                            android:layout_height="@dimen/_13pxh"
                            android:layout_marginBottom="@dimen/_5pxv"
                            android:layout_marginTop="@dimen/_4pxv"
                            android:layout_width="@dimen/_13pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_vector_amber_500"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_vector_amber_500"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtFortyFive"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_3pxh"
                            android:layout_weight="0.64"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{materialPageVM.materialPageModel.txtFortyFive}"
                            style="@style/txtPoppinsmedium15" tools:text="@string/lbl_4_5"/>
                    </LinearLayout>
                </LinearLayout>
                <TextView android:gravity="start"
                    android:id="@+id/txtDescription"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_40pxh"
                    android:layout_marginStart="@dimen/_40pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="match_parent"
                    android:text="@{materialPageVM.materialPageModel.txtDescription}"
                    style="@style/txtPoppinsregular12" tools:text="@string/msg_mulailah_perjal"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtMaterial"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_41pxh"
                    android:layout_marginStart="@dimen/_41pxh"
                    android:layout_marginTop="@dimen/_34pxv"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_6pxh"
                    android:singleLine="true"
                    android:text="@{materialPageVM.materialPageModel.txtMaterial}"
                    style="@style/txtPoppinsmedium12" tools:text="@string/lbl_material"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:drawablePadding="@dimen/_10pxh"
                    android:drawableStart="@drawable/img_play"
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnFrameThirtyThree"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_40pxh"
                    android:layout_marginStart="@dimen/_40pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_6pxh"
                    android:paddingBottom="@dimen/_12pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_20pxh"
                    android:paddingTop="@dimen/_14pxv"
                    android:singleLine="true"
                    android:textAllCaps="false" style="@style/btnSolidRoundedOutline"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:drawablePadding="@dimen/_10pxh"
                    android:drawableStart="@drawable/img_play"
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnFrameThirtyFour"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_40pxh"
                    android:layout_marginStart="@dimen/_40pxh"
                    android:layout_marginTop="@dimen/_26pxv"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_6pxh"
                    android:paddingBottom="@dimen/_12pxv"
                    android:paddingEnd="@dimen/_21pxh"
                    android:paddingStart="@dimen/_20pxh"
                    android:paddingTop="@dimen/_14pxv"
                    android:singleLine="true"
                    android:textAllCaps="false" style="@style/btnSolidRoundedOutline"/>
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearRowvectorThree"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_40pxh"
                    android:layout_marginStart="@dimen/_40pxh"
                    android:layout_marginTop="@dimen/_26pxv"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStylewhite_3">
                    <ImageView android:id="@+id/imageVectorThree"
                        android:layout_height="@dimen/_13pxh"
                        android:layout_marginBottom="@dimen/_22pxv"
                        android:layout_marginStart="@dimen/_20pxh"
                        android:layout_marginTop="@dimen/_22pxv"
                        android:layout_width="@dimen/_13pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_play"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_play"/>
                    <TextView android:id="@+id/txtDampakKesetara"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_4pxv"
                        android:layout_marginEnd="@dimen/_64pxh"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_weight="0.95"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_10pxh"
                        android:text="@{materialPageVM.materialPageModel.txtDampakKesetara}"
                        style="@style/txtPoppinsmedium12_1" tools:text="@string/msg_dampak_kesetara"/>
                </LinearLayout>
                <EditText
                    android:background="@drawable/rectangle_bg_white_a700_border_cyan_900_radius_10"
                    android:drawablePadding="@dimen/_10pxh"
                    android:drawableStart="@drawable/img_play"
                    android:ellipsize="end"
                    android:hint="@string/msg_kisah_inspirati"
                    android:id="@+id/etFrameThirtyNine"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_40pxh"
                    android:layout_marginStart="@dimen/_40pxh"
                    android:layout_marginTop="@dimen/_26pxv"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_6pxh"
                    android:paddingBottom="@dimen/_12pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_20pxh"
                    android:paddingTop="@dimen/_14pxv"
                    android:singleLine="true"
                    android:text="@={materialPageVM.materialPageModel.etFrameThirtyNineValue}"
                    android:textColorHint="@color/cyan_900"
                    style="@style/etSolidRoundedOutline_1" tools:ignore="Autofill"/>
                <EditText
                    android:background="@drawable/rectangle_bg_white_a700_border_cyan_900_radius_10"
                    android:drawablePadding="@dimen/_10pxh"
                    android:drawableStart="@drawable/img_play"
                    android:ellipsize="end"
                    android:hint="@string/msg_perubahan_sosia"
                    android:id="@+id/etFrameForty"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_40pxh"
                    android:layout_marginStart="@dimen/_40pxh"
                    android:layout_marginTop="@dimen/_26pxv"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_6pxh"
                    android:paddingBottom="@dimen/_14pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_20pxh"
                    android:paddingTop="@dimen/_12pxv"
                    android:singleLine="true"
                    android:text="@={materialPageVM.materialPageModel.etFrameFortyValue}"
                    android:textColorHint="@color/cyan_900"
                    style="@style/etSolidRoundedOutline_1" tools:ignore="Autofill"/>
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearRowsolarbookbold"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_40pxh"
                    android:layout_marginStart="@dimen/_40pxh"
                    android:layout_marginTop="@dimen/_26pxv"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStylewhite_3">
                    <ImageView android:id="@+id/imageSolarbookbold"
                        android:layout_height="@dimen/_12pxh"
                        android:layout_marginBottom="@dimen/_21pxv"
                        android:layout_marginStart="@dimen/_22pxh"
                        android:layout_marginTop="@dimen/_21pxv"
                        android:layout_width="@dimen/_10pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_solarbookbold"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_solarbookbold"/>
                    <TextView android:id="@+id/txtMemahamiEsensi"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_3pxv"
                        android:layout_marginEnd="@dimen/_37pxh"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_weight="0.96"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_10pxh"
                        android:text="@{materialPageVM.materialPageModel.txtMemahamiEsensi}"
                        style="@style/txtPoppinsmedium12_1" tools:text="@string/msg_memahami_esensi"/>
                </LinearLayout>
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearRowsolarbookboldOne"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_40pxh"
                    android:layout_marginStart="@dimen/_40pxh"
                    android:layout_marginTop="@dimen/_26pxv"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStylewhite_3">
                    <ImageView android:id="@+id/imageSolarbookboldOne"
                        android:layout_height="@dimen/_12pxh"
                        android:layout_marginBottom="@dimen/_21pxv"
                        android:layout_marginStart="@dimen/_22pxh"
                        android:layout_marginTop="@dimen/_21pxv"
                        android:layout_width="@dimen/_10pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_solarbookbold"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_solarbookbold"/>
                    <TextView android:id="@+id/txtTransformasiBu"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_3pxv"
                        android:layout_marginEnd="@dimen/_32pxh"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_weight="0.96"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_10pxh"
                        android:text="@{materialPageVM.materialPageModel.txtTransformasiBu}"
                        style="@style/txtPoppinsmedium12_1" tools:text="@string/msg_transformasi_bu"/>
                </LinearLayout>
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearRowsolarbookboldTwo"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_40pxh"
                    android:layout_marginStart="@dimen/_40pxh"
                    android:layout_marginTop="@dimen/_26pxv"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStylewhite_3">
                    <ImageView android:id="@+id/imageSolarbookboldTwo"
                        android:layout_height="@dimen/_12pxh"
                        android:layout_marginBottom="@dimen/_21pxv"
                        android:layout_marginStart="@dimen/_22pxh"
                        android:layout_marginTop="@dimen/_21pxv"
                        android:layout_width="@dimen/_10pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_solarbookbold"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_solarbookbold"/>
                    <TextView android:id="@+id/txtLangkahlangkah"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_3pxv"
                        android:layout_marginEnd="@dimen/_57pxh"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_weight="0.96"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_10pxh"
                        android:text="@{materialPageVM.materialPageModel.txtLangkahlangkah}"
                        style="@style/txtPoppinsmedium12_1" tools:text="@string/msg_langkah_langkah"/>
                </LinearLayout>
                <androidx.appcompat.widget.AppCompatButton
                    android:drawablePadding="@dimen/_10pxh"
                    android:drawableStart="@drawable/img_vector_cyan_900_10x13"
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnFrameThirtyEight"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_40pxh"
                    android:layout_marginStart="@dimen/_40pxh"
                    android:layout_marginTop="@dimen/_26pxv"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_10pxh"
                    android:paddingBottom="@dimen/_12pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_20pxh"
                    android:paddingTop="@dimen/_14pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_game_pengenalan"
                    android:textAllCaps="false"
                    style="@style/btnSolidRoundedOutline_1" tools:text="@string/lbl_game_pengenalan"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:drawablePadding="@dimen/_11pxh"
                    android:drawableStart="@drawable/img_icroundquiz"
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnFrameThirtyNineOne"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_40pxh"
                    android:layout_marginStart="@dimen/_40pxh"
                    android:layout_marginTop="@dimen/_26pxv"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_10pxh"
                    android:paddingBottom="@dimen/_12pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_21pxh"
                    android:paddingTop="@dimen/_14pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_kuis_pengenalan"
                    android:textAllCaps="false"
                    style="@style/btnSolidRoundedOutline_1" tools:text="@string/lbl_kuis_pengenalan"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnGetStarted"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_40pxh"
                    android:layout_marginStart="@dimen/_40pxh"
                    android:layout_marginTop="@dimen/_64pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_14pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_14pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_get_started"
                    android:textAllCaps="false"
                    style="@style/btnGradientRounded" tools:text="@string/lbl_get_started"/>
            </LinearLayout>
        </ScrollView>
    </FrameLayout>
</layout>
